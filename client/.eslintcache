ttp://localhost:3001/update\", { name: newName, id: id }).then(\n      (response) => {\n        setStudentList(\n          studentList.map((val) => {\n            return val.id == id\n              ? {\n                  id: val.id,\n                  name: val.name,\n                  age: val.age,\n                  faculty: val.faculty,\n                  major: val.major,\n                }\n              : val;\n          })\n        );\n      }\n    );\n  };\n\n  const deleteStudent = (id) => {\n    Axios.delete(`http://localhost:3001/delete/${id}`).then((response) => {\n      setStudentList(\n        studentList.filter((val) => {\n          return val.id != id;\n        })\n      );\n    });\n  };\n\n  return (\n    <div className=\"App-container\">\n      <h1 className=\"head\">Student Information</h1>\n      <div className=\"information\">\n        <from action=\"\">\n          <div className=\"mb-3\">\n            <label htmlFor=\"name\" className=\"from-label\">\n              Name:\n            </label>\n            <input\n              type=\"text\"\n              className=\"from-control\"\n              placeholder=\"Enter name\"\n              onChange={(event) => {\n                setName(event.target.value);\n              }}\n            />\n          </div>\n          <div className=\"mb-3\">\n            <label htmlFor=\"age\" className=\"from-label\">\n              Age:\n            </label>\n            <input\n              type=\"number\"\n              className=\"from-control\"\n              placeholder=\"Enter age\"\n              onChange={(event) => {\n                setAge(event.target.value);\n              }}\n            />\n          </div>\n          <div className=\"mb-3\">\n            <label htmlFor=\"faculty\" className=\"from-label\">\n              Faculty:\n            </label>\n            <input\n              type=\"text\"\n              className=\"from-control\"\n              placeholder=\"Enter faculty\"\n              onChange={(event) => {\n                setFaculty(event.target.value);\n              }}\n            />\n          </div>\n          <div className=\"mb-3\">\n            <label htmlFor=\"major\" className=\"from-label\">\n              Major:\n            </label>\n            <input\n              type=\"text\"\n              className=\"from-control\"\n              placeholder=\"Enter major\"\n              onChange={(event) => {\n                setMajor(event.target.value);\n              }}\n            />\n          </div>\n          <button className=\"btn btn-success\" onClick={addStudent}>\n            Register\n          </button>\n        </from>\n      </div>\n      <hr />\n      <div className=\"register\">\n        <button className=\"btn btn-primary\" onClick={getStudent}>\n          Show Student\n        </button>\n        <br></br>\n        {studentList.map((val, key) => {\n          return (\n            <div className=\"student card\">\n              <div className=\"card-body  text-left\">\n                <p className=\"card-text\">Name: {val.name}</p>\n                <p className=\"card-text\">Age: {val.age}</p>\n                <p className=\"card-text\">Faculty: {val.faculty}</p>\n                <p className=\"card-text\">Major: {val.major}</p>\n                <div className=\"d-flex\">\n                  <input\n                    type=\"text\"\n                    type=\"text\"\n                    placeholder=\"Change Name\"\n                    onChange={(event) => {\n                      setNewName(event.target.value);\n                    }}\n                  />\n                  <button\n                    className=\"btn btn-warning\"\n                    onClick={() => {\n                      updateStudentName(val.id);\n                    }}\n                  >\n                    Update\n                  </button>\n                </div>\n                <br></br>\n                <button\n                  